{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run vercel-build",
        "outputDirectory": "."
      }
    },
    {
      "src": "storybook-static/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/storybook",
      "destination": "/storybook-static/index.html"
    },
    {
      "source": "/storybook/:path*",
      "destination": "/storybook-static/:path*"
    }
  ]
} 