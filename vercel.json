{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "storybook-static",
  "framework": "vite",
  "rewrites": [
    { 
      "source": "/",
      "destination": "/dist/index.html"
    },
    {
      "source": "/assets/:path*",
      "destination": "/dist/assets/:path*"
    },
    {
      "source": "/storybook",
      "destination": "/index.html"
    },
    {
      "source": "/storybook/(.*)",
      "destination": "/$1"
    }
  ]
} 